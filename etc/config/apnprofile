config rule 'visible'
	option service ''
	option apn 'VSBLINTERNET'
	option protocol 'qcm'
	option pincode ''
	option dialnumber ''
	option username ''
	option password ''
	option auth ''
	option description 'VISIBLE'
	option apn_use '3'
	option apns '["VSBLIMS","VSBLADMIN","","VSBLAPP"]'

