
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
	option band '2g'
	option country 'US'
	option disabled '0'
	option htmode 'HT40'
	option legacy_rates '0'
	option channel '6'
	option hwmode '11g'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option wds '1'
	option isolate '0'
	option ifname 'wlan0'
	option hidden '0'
	option key '21energyminer'
	option ssid 'TollGate'

config wifi-iface 'guest2g'
	option device 'radio0'
	option network 'guest'
	option mode 'ap'
	option ifname 'wlan0-1'
	option key 'goodlife'
	option guest '1'
	option wds '1'
	option isolate '1'
	option disabled '0'
	option ssid 'TollGate'
	option hidden '0'
	option encryption 'none'

config wifi-iface 'sta'
	option mode 'sta'
	option device 'radio0'
	option network 'wwan'
	option ssid 'NOS-F7D6'
	option bssid '60:6C:63:7B:F7:D6'
	option key 'WLXAU926'
	option encryption 'psk-mixed'
	option ifname 'wlan-sta0'

