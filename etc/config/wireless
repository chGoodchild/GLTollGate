
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
	option band '2g'
	option country 'US'
	option disabled '0'
	option htmode 'HT40'
	option legacy_rates '0'
	option channel '6'
	option hwmode '11g'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option key 'goodlife'
	option wds '1'
	option isolate '0'
	option ifname 'wlan0'
	option ssid 'TollGate'
	option hidden '0'
	option encryption 'none'

config wifi-iface 'guest2g'
	option device 'radio0'
	option network 'guest'
	option mode 'ap'
	option ifname 'wlan0-1'
	option encryption 'psk2'
	option key 'goodlife'
	option ssid 'GL-AR300M-1a0-Guest'
	option guest '1'
	option disabled '1'
	option wds '1'
	option isolate '1'

config wifi-iface 'sta'
	option mode 'sta'
	option device 'radio0'
	option network 'wwan'
	option ssid 'COWORK'
	option bssid 'F6:9F:C2:FA:75:A6'
	option key 'coworking'
	option encryption 'psk-mixed'
	option ifname 'wlan-sta0'

